@model QLTHPT.Models.COSOVATCHAT

@{
    ViewBag.Title = "Delete";
}

<div class="data-table-area mg-b-15">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="sparkline13-list shadow-reset">
                    <div class="sparkline13-hd">
                        <div class="main-sparkline13-hd">
                            <h4>
                                <strong>
                                    Xóa Thông Tin Cơ Sở Vật Chất
                                </strong>
                            </h4>

                        </div>
                    </div>
                    <h5>Bạn có chắc muốn xóa ?</h5>
                    <div class="sparkline10-graph">
                        <div class="card-body">
                            <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>
                                            Mã
                                        </th>
                                        <th>
                                            Thiết Bị Dạy Học
                                        </th>
                                        <th>
                                            Thiết Bị Giảng Dạy
                                        </th>
                                        <th>
                                            Phòng Học
                                        </th>
                                        <th>
                                            Số Lượng
                                        </th>
                                        <th>
                                            Mô Tả
                                        </th>
                                    </tr>
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(model => model.CSVC_MA)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.THIETBIDAYHOC.TBDH_TEN)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.THIETBIGIANGDAY.TBGD_TEN)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.PHONGHOC.PH_TEN)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.CSVC_SOLUONG)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(model => model.TINHTRANG.TT_MOTA)
                                        </td>
                                    </tr>
                                </thead>

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <input type="submit" value="Xóa" class="btn btn-default" /> |
        <a href="@Url.Action("Index", "COSOVATCHATs")" class=" btn btn-custon-rounded-three btn primary"><i class="fa fa-undo"></i></a>
    </div>
}